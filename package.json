    "dotenv": "^16.0.3",
    "ws": "^8.8.0",
    "uuid": "^9.0.0"
  },
  "homepage": "https://github.com/thirdmadman/nodejs-2023-q2-websockets-ui#readme",
  "devDependencies": {
    "@types/uuid": "^9.0.2",
    "@types/ws": "^8.5.5",
    "@types/node": "^18.11.9",
