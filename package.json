{
  "name": "nodejs-2023-q2-websockets-ui",
  "version": "1.0.0",
  "description": "Template for the WebSocket remote control task",
  "main": "index.js",
  "private": true,
  "scripts": {
    "prod:build": "cross-env NODE_ENV=production && rm -rf dist && tsc",
    "prod:start": "node dist/listen",
    "dev": "cross-env NODE_ENV=development ts-node-dev --no-notify --respawn --transpile-only src/listen.ts",
    "prod": "npm run prod:build && npm run prod:start-no-bundle"
  },
